{
  "expo": {
    "name": "SMDcarpool",
    "description": "This project is really great.",
    "slug": "smd_carpool",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "android"
    ],
	"ios": {
      "bundleIdentifier": "com.teamcais.smdcarpool"
    },
    "android": {
      "package": "com.teamcais.smdcarpool",
	  "permissions": [
		  "MANAGE_DOCUMENTS",
		  "READ_EXTERNAL_STORAGE",
		  ],
    },
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#485b62"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidStatusBar": {
      "backgroundColor": "#3f3f3f"
    }
  }
}